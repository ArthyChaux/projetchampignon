{
  "expo": {
    "name": "ChampiScan",
    "slug": "ChampiScan",
    "version": "pre_alfa",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/fond.jpg",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.arthychaux.ChampiScan"
    },
    "extra": {
      "eas": {
        "projectId": "8b7f8167-7bce-4076-ac71-08a3b8506116"
      }
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "The app accesses your camera to let you share with your friends."
        }
      ]
    ]
  }
}
